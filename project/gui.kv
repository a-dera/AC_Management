####--------------------------Login Popup Class------------------------------------##############

<LoginPopup>:
	

	size_hint: (0.4, 0.7)
	
	auto_dismiss: True

	background: "media/images/login3.png"
	canvas:
		Color:
			rgba: (0,0,1,0)
		Rectangle:
			pos: self.pos
			size: self.size
     
   #####-------------------Parent Boxlayout----------------------------######################

	BoxLayout:
		orientation: 'vertical'
		canvas:
			Color:
				rgba: (1,0,1,0)
			Rectangle:
				pos: self.pos
				size: self.size

		#------------------------title------------------------------------------#############
		BoxLayout:
			size_hint: (1, .1)

			canvas:
				Color:
					rgba: (1,0,1,0)
				Rectangle:
					pos: self.pos
					size: self.size

			Label:
				id: loginTitle
				text: ""
				font_size: dp(35)
				bold: True
				color: (0, 0, 0, 1)


		AnchorLayout:
			size_hint: (1, 0.9)
			canvas:
				Color:
					rgba: (1,0,0.5,0)
				Rectangle:
					pos: self.pos
					size: self.size

			#####------------------contains textInputs------------------------------#
			GridLayout:
				size_hint: (0.6, 0.9)
				canvas:
					Color:
						rgba: (0.5,0,1,0)
					Rectangle:
						pos: self.pos
						size: self.size


				rows: 4
				cols: 1
				spacing: (0, 2)

				#------------login failure message------------------------------------#
				BoxLayout:
					size_hint: (1, 0.1)
					canvas:
						Color:
							rgba: (0.5,0.3,1,0)
						Rectangle:
							pos: self.pos
							size: self.size

					Label:
						id: warningInfo
						text: ''
						color: (1, 0, 0, 1)

				#-----------------------username-----------------#
				AnchorLayout:
					size_hint: (1, 0.25)
					canvas:
						Color:
							rgba: (0.3,0,0.2,0)
						Rectangle:
							pos: self.pos
							size: self.size

					BoxLayout:
						size_hint: (1, None)
						height: 40
						canvas:
							Color:
								rgba: (0,0,0,1)
							Rectangle:
								pos: self.pos
								size: self.size

						#--------username icon-----------##
						AnchorLayout:
							size_hint: (None, 1)
							width: 40
							BoxLayout:
								size_hint: (None, None)
								height: 30
								width: 30
								canvas:
									Color:
										rgba: (1,1,1,0.7)
									Rectangle:
										pos: self.pos
										size: self.size
										source: "media/images/user_icon.png"

						#---------username input----------#
						BoxLayout:
							size_hint: (1,1)
							canvas:
								Color:
									rgba: (0.5,0.2,0.3,0)
								Rectangle:
									pos: self.pos
									size: self.size
							TextInput:
								id: username_
								size_hint: (1, 1)
								hint_text:"Username"
								background_color: (0, 0, 0, 1)
								hint_text_color: (1,1,1,0.5)
								font_size: dp(20)
								padding: self.size[0]*0.1,self.size[1]/6
								foreground_color: (1, 1, 1, 1)
								write_tab: False
								multiline: False
								cursor_color:(1,1,1,1)
								on_text_validate:
									x= root.login(root.ids.username_.text, root.ids.password_.text,)
									if x: app.root.transition= SlideTransition(direction='left')
									if x and root.ids.loginTitle.text=="User Login": app.root.current= "userScreen"
									if x and root.ids.loginTitle.text=="Admin Login": app.root.current= "adminScreen"
									if x and root.ids.loginTitle.text=="Admin Login": app.root.ids.adminScreen.onStartAdminScr()




				#-------------------password---------------------------#
				AnchorLayout:
					size_hint: (1, 0.25)
					anchor_y: 'top'
					canvas:
						Color:
							rgba: (0.5,0,0.3,0)
						Rectangle:
							pos: self.pos
							size: self.size

					BoxLayout:
						size_hint: (1, None)
						height: 40
						canvas:
							Color:
								rgba: (0,0,0,1)
							Rectangle:
								pos: self.pos
								size: self.size

						#--------password icon-----------##
						AnchorLayout:
							size_hint: (None, 1)
							width: 40
							BoxLayout:
								size_hint: (None, None)
								height: 30
								width: 30
								canvas:
									Color:
										rgba: (1,1,1,0.7)
									Rectangle:
										pos: self.pos
										size: self.size
										source: "media/images/pass_icon.png"

						#---------password input----------#
						BoxLayout:
							size_hint: (1,1)
							canvas:
								Color:
									rgba: (0,0,0,1)
								Rectangle:
									pos: self.pos
									size: self.size
							TextInput:
								id: password_
								size_hint: (1, 1)
								hint_text:"Password"
								password: True
								password_mask:"*"
								background_color: (0, 0, 0, 1)
								hint_text_color: (1,1,1,0.5)
								font_size: dp(20)
								padding: self.size[0]*0.1,self.size[1]/6
								foreground_color: (1, 1, 1, 1)
								write_tab: False
								multiline: False
								cursor_color:(1,1,1,1)
								on_text_validate:
									x=root.login(root.ids.username_.text, root.ids.password_.text,)
									if x: app.root.transition= SlideTransition(direction='left')
									if x and root.ids.loginTitle.text=="User Login": app.root.current= "userScreen"
									if x and root.ids.loginTitle.text=="Admin Login": app.root.current= "adminScreen"
									if x and root.ids.loginTitle.text=="Admin Login": app.root.ids.adminScreen.onStartAdminScr()



				#-------------------Contains login button and fotgot password----------------------------------#
				BoxLayout:
					orientation: 'vertical'
					size_hint: (1, 0.4)
					canvas:
						Color:
							rgba: (1,0,1,0)
						Rectangle:
							pos: self.pos
							size: self.size

					#-------------Login Btn Area----------------------------------#
					AnchorLayout:
						size_hint: (1, 0.5)
						canvas:
							Color:
								rgba: (0.5,0.5,0.5,0)
							Rectangle:
								pos: self.pos
								size: self.size

						#-----------------login btn-------------#
						CustomButton:
							size_hint: (1, .5)
							border: (10, 10, 10, 10)
							background_normal: 'media/images/but_blue_down.png'
							background_down: 'media/images/but_blue_norm.png'
							#canvas:
							#	Color:
							#		rgba: (0,0,1,0)
							#	Rectangle:
							#		pos: self.pos
							#		size: self.size
							text: "Login"
							on_release:
								x= root.login(root.ids.username_.text, root.ids.password_.text,)
								if (x and root.ids.loginTitle.text=="User Login"): app.root.current= "userScreen"
								if (x and root.ids.loginTitle.text=="Admin Login"): app.root.current= "adminScreen"
								if x and root.ids.loginTitle.text=="Admin Login": app.root.ids.adminScreen.onStartAdminScr()


					#--------------forgot password area---------------------------#
					AnchorLayout:
						size_hint: (1, 0.5)
						canvas:
							Color:
								rgba: (0.3,0.3,0.3,0)
							Rectangle:
								pos: self.pos
								size: self.size

						Button:
							size_hint: (0.5, 0.3)
							background_color: (0, 0, 0, 0)
							canvas.before:
								Color:
									rgba: (0,0,1,0)
								Rectangle:
									pos: self.pos
									size: self.size

							text:"Forgot Details?"
							color: (0, 0, 1, 1)




