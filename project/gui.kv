#-------------------ProfilePage---------------------#
<ProfilePage>:
	name: 'profilepage'
	FloatLayout:
		canvas:
			Color:
				rgba: C(root.color)
			Rectangle:
				size: self.size
				pos: self.pos

		BoxLayout:
			orientation: 'vertical'
			spacing: dp(5)

			BoxMinimumHeight:
				padding:0,2,0,0

				Widget:
					size_hint: None, 1
					width: 20

